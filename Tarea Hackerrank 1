SinglyLinkedListNode* insertNodeAtTail(SinglyLinkedListNode* head, int data) {
SinglyLinkedListNode *tmp = malloc(sizeof(SinglyLinkedListNode));
tmp->next = NULL;
tmp->data = data;
if(head == NULL)
{
head = tmp;
return head;
}
SinglyLinkedListNode *actual = head;
while(actual->next != NULL)
{
actual = actual->next;
}
actual->next = tmp;
return head;
}
