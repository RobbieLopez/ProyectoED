}

int main(){
  int num;
  int pro[9];
  printf(" Un mercadito instrucciones:");
  printf("\n 1.-¿Cuantos productos agregar a la cansta?: ");
  scanf("%d",&num);
  printf("\n 1.-Leche    |6.-Crema");
  printf("\n 2.-Huevo    |7.-Cereal");
  printf("\n 3.-Arroz    |8.-Aceite");
  printf("\n 4.-Lechuga  |9.-Tortillas");
  printf("\n 5.-Jitomate |10.-Mayonesa");
