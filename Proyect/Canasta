#include <stdio.h>
#include <stdlib.h>

typedef struct Arbol{
  int dato;
  struct Arbol *derecha;
  struct Arbol *izquierda;
}Arbol;

Arbol *raiz=NULL;


void insertarArbol(int pro[9],int num){
  for(int j=0;j<num;j++){
      Arbol *nuevo=(Arbol*)malloc(sizeof(Arbol*));
      dato=pro[j];
      nuevo=nuevo->dato;
      if(raiz==NULL){
        raiz==nuevo->dato;
        raiz->derecha=NULL;
        raiz->izqueira=NULL;
      }
